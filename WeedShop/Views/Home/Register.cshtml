@using Infrastructuur.Models
@using WeedShop.Controllers
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    var failedRegisterLoginMessage = ViewData["LoginFailed"] as string;
}

@model UserEntity;

<form asp-action="ValidateRegister" method="post">

    <label asp-for="@Model.FirstName"></label>
    <input type="text" asp-for="@Model.FirstName"/>

    <label asp-for="@Model.LastName"></label>
    <input type="text" asp-for="@Model.LastName"/>


    <label asp-for="@Model.Password"></label>
    <input type="password" asp-for="@Model.Password"/>

       <label asp-for="@Model.Email"></label>
    <input type="email" asp-for="@Model.Email"/>

    <button type="submit">Submit</button>
</form>
@if(!string.IsNullOrEmpty(@HomeController.userFailedRegister))
{
    <p>@HomeController.userFailedRegister</p>
 
}
